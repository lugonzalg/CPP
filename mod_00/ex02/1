#include "Account.hpp"
#include <iostream>

static int Account::_nbAccounts;

Account::Account( int initial_deposit )
{
	initial_deposit = 1;
	Account::_nbAccounts = 0;
}

Account::~Account( void )
{
	std::cout << "qwerty";
}

void	Account::displayAccountsInfos( void )
{
	size_t	i;	

	i = -1;
	while (++i < Account::_nbAccounts)
		std::cout << "index:" << i;
}
